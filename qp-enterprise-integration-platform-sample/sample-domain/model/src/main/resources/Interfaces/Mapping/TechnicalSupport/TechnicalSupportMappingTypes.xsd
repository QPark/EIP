	<complexType name="BookType.titleConcatenationType">
		<complexContent>
			<extension base="MPmp:ComplexMappingType">
				<sequence>
					<element name="techSupportBook" type="ISSTechSupport:BookType"/>
					<element name="issBook" type="ISSLib:BookType"/>
