		</complexContent>
	</complexType>

	<element name="GetFlowReportRequest"
		type="ISStecsuppm:GetFlowReportRequestType" />
	<element name="GetFlowReportResponse"
		type="ISStecsuppm:GetFlowReportResponseType" />

</schema>
