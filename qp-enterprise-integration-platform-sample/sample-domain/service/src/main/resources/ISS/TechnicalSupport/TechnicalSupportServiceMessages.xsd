	<complexType name="GetFlowReportRequestType">
		<annotation>
			<documentation>
				@ServiceConsumer tbd
				@ServiceImplementor ESB
				@ServiceDataProvider ESB
				@ServiceDescription Get the flow log messages reported.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="EIPbasem:RequestMessage">
				<sequence>
