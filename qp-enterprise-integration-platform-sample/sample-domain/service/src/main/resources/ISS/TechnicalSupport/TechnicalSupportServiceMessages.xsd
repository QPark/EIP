	<complexType name="GetAggregatedReferenceDataRequestType">
		<annotation>
			<documentation>@ServiceConsumer
tbd
				@ServiceImplementor
ESB
				@ServiceDataProvider
ESB
				@ServiceDescription
Example of the an aggregated GetReferenceData call.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="EIPctym:RequestMessage">
				<sequence>
					<element name="criteria" type="EIPcty:ReferenceDataCriteriaType">
						<annotation>
							<documentation>Reference data search criteria</documentation>
						</annotation>
					</element>
				</sequence>
				</extension>
		</complexContent>
	</complexType>
	<complexType name="GetAggregatedReferenceDataResponseType">
		<annotation>
			<documentation>Aggregated reference data from ESb enumerations.</documentation>
		</annotation>
		<complexContent>
			<extension base="EIPctym:ResponseMessage">
				<sequence>
					<element name="referenceData" type="EIPcty:ReferenceDataType" minOccurs="0" maxOccurs="unbounded">
						<annotation>
							<documentation>Reference data</documentation>
						</annotation>
					</element>
				</sequence>
				</extension>
		</complexContent>
	</complexType>

	<element name="GetAggregatedReferenceDataRequest"
		type="ISStecsuppm:GetAggregatedReferenceDataRequestType" />
	<element name="GetAggregatedReferenceDataResponse"
		type="ISStecsuppm:GetAggregatedReferenceDataResponseType" />
	
	<complexType name="GetSystemUserReportRequestType">
		<annotation>
			<documentation>@ServiceConsumer tbd
				@ServiceImplementor ESB
				@ServiceDataProvider ESB
				@ServiceDescription Example of the an aggregated GetReferenceData call.
			</documentation>
		</annotation>
		<complexContent>
			<extension base="EIPctym:RequestMessage">
				<sequence>
					<element name="criteria" type="ISStecsup:SystemUserReportCriteriaType"/>
				</sequence>
				</extension>
		</complexContent>
	</complexType>
	<complexType name="GetSystemUserReportResponseType">
		<complexContent>
			<extension base="EIPctym:ResponseMessage">
				<sequence>
					<element name="report" type="ISStecsup:SystemUserReportType" minOccurs="0" maxOccurs="unbounded"/>
				</sequence>
				</extension>
		</complexContent>
	</complexType>

	<element name="GetSystemUserReportRequest"
		type="ISStecsuppm:GetSystemUserReportRequestType" />
	<element name="GetSystemUserReportResponse"
		type="ISStecsuppm:GetSystemUserReportResponseType" />

	<complexType name="GetFlowReportRequestType">
		<annotation>
			<documentation>@ServiceConsumer tbd
				@ServiceImplementor ESB
				@ServiceDataProvider ESB
				@ServiceDescription Example of the an aggregated GetReferenceData call.
			</documentation>
		</annotation>
		<complexContent>
