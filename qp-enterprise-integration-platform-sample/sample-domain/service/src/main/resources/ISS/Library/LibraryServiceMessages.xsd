<schema xmlns="http://www.w3.org/2001/XMLSchema"
	xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	
	xmlns:ISScty="http://www.samples/CommonTypes"
	xmlns:ISSlib="http://www.samples.com/Library/LibraryTypes"
	
	xmlns:EIPctym="http://www.qpark.com/Common/CommonServiceMessages"
	xmlns:ISSlibm="http://www.sample.com/Library/LibraryServiceMessages"
	
	targetNamespace="http://www.sample.com/Library/LibraryServiceMessages"
	elementFormDefault="qualified" jaxb:version="2.0">


	<import 
	    schemaLocation="http://www.samples.com/model/ISS/Library/Library.xsd" 
	    namespace="http://www.samples.com/Library/LibraryTypes"></import>
	<import
		schemaLocation="../../EIP/Common/CommonServiceMessages.xsd"
		namespace="http://www.qpark.com/Common/CommonServiceMessages">
	</import>
	<import
		schemaLocation="http://www.samples.com/model/ISS/Common/Common.xsd"
		namespace="http://www.samples.com/CommonTypes">
	</import>

	<annotation>
		<appinfo>
			<jaxb:schemaBindings>
				<jaxb:package
					name="com.samples.platform.service.library.msg" />
			</jaxb:schemaBindings>
		</appinfo>
		<documentation>
			Message definitions of the library web service.
		</documentation>
	</annotation>

	<complexType name="CreateBookRequestType">
		<complexContent>
			<extension base="EIPctym:RequestMessage">
				<sequence>
					<element name="book" type="ISSlib:BookType" maxOccurs="unbounded" minOccurs="1"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="CreateBookResponseType">
		<complexContent>
			<extension base="EIPctym:ResponseMessage">
				<sequence>
					<element name="book" type="ISSlib:BookType" maxOccurs="unbounded" minOccurs="0"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<element name="CreateBookRequest" type="ISSlibm:CreateBookRequestType"></element>
    <element name="CreateBookResponse" type="ISSlibm:CreateBookResponseType"></element>
</schema>