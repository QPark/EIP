 *     Bernhard Hausen - Initial API and implementation
-->
			Message definitions of the library web service.
		</documentation>
	</annotation>

	<complexType name="CreateBookRequestType">
		<complexContent>
			<extension base="EIPctym:RequestMessage">
				<sequence>
					<element name="book" type="ISSlib:BookType" maxOccurs="unbounded" minOccurs="1"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="CreateBookResponseType">
		<complexContent>
			<extension base="EIPctym:ResponseMessage">
				<sequence>
					<element name="book" type="ISSlib:BookType" maxOccurs="unbounded" minOccurs="0"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<element name="CreateBookRequest" type="ISSlibm:CreateBookRequestType"></element>
    <element name="CreateBookResponse" type="ISSlibm:CreateBookResponseType"></element>

	<complexType name="GetBookRequestType">
		<complexContent>
			<extension base="EIPctym:RequestMessage">
				<sequence>
					<element name="criteria" type="ISSlib:BookCriteriaType" maxOccurs="1" minOccurs="1"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<complexType name="GetBookResponseType">
		<complexContent>
			<extension base="EIPctym:ResponseMessage">
				<sequence>
					<element name="book" type="ISSlib:BookType" maxOccurs="unbounded" minOccurs="0"></element>
				</sequence>
			</extension>
		</complexContent>
	</complexType>

	<element name="GetBookRequest" type="ISSlibm:GetBookRequestType"></element>
    <element name="GetBookResponse" type="ISSlibm:GetBookResponseType"></element>

</schema>